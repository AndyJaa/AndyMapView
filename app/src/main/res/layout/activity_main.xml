<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:andy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jaa.andy.com.demo.MainActivity">

    <com.andy.jaa.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <jaa.andy.com.demo.view.MyMapView
        android:visibility="gone"
        android:id="@+id/imageView"
        andy:src="@mipmap/map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


    <jaa.andy.com.demo.view.AndyView
        android:visibility="gone"
        android:id="@+id/andyView"
        android:layout_width="200dp"
        android:layout_height="60dp"
        andy:titleTextColor="@color/colorAccent"
        andy:titleTextContent="0707"
        andy:titleTextSize="18sp"/>

</android.support.constraint.ConstraintLayout>
